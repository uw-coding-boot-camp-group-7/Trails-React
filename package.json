{
  "name": "trails-react",
  "version": "1.0.0",
  "description": "Hiking Trail finding application built on react",
  "main": "index.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uw-coding-boot-camp-group-7/Trails-React.git"
  },
  "keywords": [
    "Trails",
    "Hiking",
    "Activities",
    "Passports"
  ],
  "author": "Mario, Josh, Joonwoo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/uw-coding-boot-camp-group-7/Trails-React/issues"
  },
  "homepage": "https://github.com/uw-coding-boot-camp-group-7/Trails-React#readme",
  "dependencies": {
    "express": "^4.16.4"
  }
  
}
